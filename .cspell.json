{
  "version": "0.2",
  "language": "en",
  "ignorePaths": [
    "**/node_modules/**",
    "bun.lockb",
    "cspell.json",
    "package.json",
    "**.stories.tsx",
    "gen/**",
    "playwright-report/**",
    "**/prisma/schema.prisma",
    "**/prisma/migrations/**",
    "packages/shared/prisma/seed",
    "**/tsconfig.tsbuildinfo",
    "docker/**",
    "docker-compose.yml",
    "packages/shared/constants/env/**"
  ],
  "allowCompoundWords": true,
  "dictionaryDefinitions": [],
  "ignoreRegExpList": ["/\\d+\\.\\d+\\.\\d+/"],
  "ignoreWords": [
    "bun",
    "workspace",
    "GlobalRegistrator",
    "registrator",
    "Elysia",
    "elysia",
    "elysiajs",
    "data-testid",
    "sonner",
    "commmune",
    "Enquete",
    "enquete",
    "vhidden",
    "signup",
    "idaas",
    "mypage",
    "vaul",
    "embla",
    "unparse",
    "date-testid"
  ],
  "patterns": [
    {
      "name": "camelCase",
      "pattern": "[a-z]+(?:[A-Z][a-z]+)+"
    }
  ],
  "words": ["gcloud", "kaniko"]
}
