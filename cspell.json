{
  "version": "0.2",
  "language": "en",
  "ignorePaths": [
    "**/node_modules/**",
    "bun.lockb",
    "cspell.json",
    "package.json",
    "**.stories.tsx",
    "gen/**",
    "playwright-report/**",
    "**/prisma/schema.prisma",
    "**/prisma/migrations/**",
    "packages/shared/prisma/seed",
    "**/tsconfig.tsbuildinfo",
    "docker/**",
    "docker-compose.yml"
  ],
  "allowCompoundWords": true,
  "dictionaryDefinitions": [],
  "ignoreRegExpList": ["/\\d+\\.\\d+\\.\\d+/"],
  "ignoreWords": [],
  "patterns": [
    {
      "name": "camelCase",
      "pattern": "[a-z]+(?:[A-Z][a-z]+)+"
    }
  ]
}
