    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "jsdom": "^24.0.0",
    "lefthook": "^1.6.5",
    "markdownlint-cli2": "^0.12.1",
    "markuplint": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "papaparse": "^5.4.1",
    "plop": "4.0.1",
    "react-docgen-typescript-plugin": "^1.0.5",
    "storybook": "^7.6.17",
    "storycap": "^4.2.0",
    "typescript": "^5.3.3",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.3.1"
  },
