    "build-storybook": "storybook build",
    "format": "prettier ./src --write",
    "plop": "plop",
    "e2e": "playwright test",
    "depcheck": "depcheck",
    "gen": "run-p gen:*",
    "gen:i18n": "mkdir -p gen/i18n && bun ./i18n/index.js"
  },
  "devDependencies": {
    "@markuplint/jsx-parser": "^4.3.0",
    "@markuplint/react-spec": "^4.1.0",
