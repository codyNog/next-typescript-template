{
  "name": "next-ts",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@sentry/nextjs": "^7.105.0",
    "@tanstack/react-query": "^5.25.0",
    "next": "^14.1.2",
    "next-international": "^1.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "valibot": "^0.29.0"
  },
  "scripts": {
    "preinstall": "npx only-allow bun",
    "postinstall": "bun gen",
